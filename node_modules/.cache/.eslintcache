Y = process.env.REACT_APP_MOVIEDB_API_KEY;\n\tuseEffect(() => {\n\t\tfetchMovie();\n\t}, []);\n\n\tconst fetchMovie = async () => {\n\t\tconst { data } = await axios.get(\n\t\t\t`https://api.themoviedb.org/3/movie/${match.params.id}?api_key=${API_KEY}`\n\t\t);\n\t\tsetMovieDetails(data);\n\t\t// console.log(data);\n\t};\n\n\treturn (\n\t\t<div className='details-container'>\n\t\t\t<div className='details-wrapper' key={movieDetals.id}>\n\t\t\t\t<h2>{movieDetals.title}</h2>\n\t\t\t\t<div className='img-description'>\n\t\t\t\t\t<img src={`${images}${movieDetals.poster_path}`} alt='none'></img>\n\t\t\t\t\t<div className='year-rating'>\n\t\t\t\t\t\t<p>Rating: {movieDetals.vote_average}</p>\n\t\t\t\t\t\t<p>Year: {movieDetals.release_date}</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<p>{movieDetals.overview}</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t);\n}\n","/Users/deprincio/Documents/2. Web Dev/Practice/React/Movie App/movie_app_v_1.0/src/Popular.js",[],"/Users/deprincio/Documents/2. Web Dev/Practice/React/Movie App/movie_app_v_1.0/src/Home.js",["65"],"import React from 'react';\nimport Popular from './Popular';\nimport Toprated from './Toprated';\nimport Upcoming from './Upcoming';\nimport './App.css';\nimport tv from './images/tv.png';\n\nexport default function Home() {\n\treturn (\n\t\t<div>\n\t\t\t<div className='home-banner'>\n\t\t\t\t<div className='enjoy-your-tv'>\n\t\t\t\t\t<h1>Enjoy Your TV.</h1>\n\t\t\t\t\t<p>\n\t\t\t\t\t\tWatch on Smart TVs, Playstation, Xbox, Chromecast, Apple TV, Blu-ray\n\t\t\t\t\t\tplayers, and more.\n\t\t\t\t\t</p>\n\t\t\t\t</div>\n\t\t\t\t<div className='img-container'>\n\t\t\t\t\t<img src={tv} />\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<Popular />\n\t\t\t<Toprated />\n\t\t\t<Upcoming />\n\t\t</div>\n\t);\n}\n","/Users/deprincio/Documents/2. Web Dev/Practice/React/Movie App/movie_app_v_1.0/src/Search.js",["66","67"],"import React, { useEffect, useState } from 'react';\nimport { useSelector } from 'react-redux';\nimport axios from 'axios';\n\ni